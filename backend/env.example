# Spotube Environment Configuration
# Copy this file to .env and update with your actual values

# OAuth Credentials (optional - can be configured via setup wizard UI)
# Spotify Developer Console: https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Google Cloud Console: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Server Configuration
PUBLIC_URL=http://127.0.0.1:8090
FRONTEND_URL=http://localhost:5173
PORT=8090

# Logging Level (optional)
# Options: debug, info, warn, error
LOG_LEVEL=debug

# Development Settings
# Set to true to enable development features
DEV_MODE=true 